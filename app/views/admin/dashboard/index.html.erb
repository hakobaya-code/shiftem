<%= render 'layouts/headerhome' %>
<div class="container">

<h2>管理者ページ</h2>

<%= form_tag(attendances_path, method: :post) do %>
	<%= submit_tag "attendance", name: "attendance_type", value: "出勤" %>
	<%= submit_tag "break_start", name: "attendance_type", value: "休憩" %>
	<%= submit_tag "break_end", name: "attendance_type", value: "戻り" %>
	<%= submit_tag "leave", name: "attendance_type", value: "退勤" %>
<% end %>

<%= button_to "ログアウト", destroy_user_session_path, method: :delete %>

</div>